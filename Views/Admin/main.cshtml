<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title></title>
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <link href="http://fonts.googleapis.com/css?family=Varela" rel="stylesheet" />
    <link href="~/Content/default.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Content/fonts.css" rel="stylesheet" type="text/css" media="all" />

    <!--[if IE 6]><link href="default_ie6.css" rel="stylesheet" type="text/css" /><![endif]-->

</head>
<body>
    <div id="wrapper">
        <div id="header-wrapper">
            <div id="header" class="container">
                <div id="logo">
                    <h1><a href="#"> New Hope</a></h1>
                </div>
                <div id="menu">
                    <ul>
                        <li class="active"><a href="~/Home/index" accesskey="1" title="">HOME</a></li>
                        <li><a href="~/User/donate" accesskey="2" title="">DONATE BLOOD</a></li>
                        <li><a href="~/User/find" accesskey="3" title="">FIND BLOOD</a></li>
                        <li><a href="~/Home/faq" accesskey="4" title="">Faq</a></li>
                        <li><a href="~/Home/contact" accesskey="5" title="">Contact Us</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div id="banner">
            <div class="container">
                <div class="title">
                    <h2>Blood Donation Site</h2>
                    <span class="byline">Donate Blood to save lives</span>
                </div>
                <ul class="actions">
                    <li><a href="#" class="button">thanks</a></li>
                </ul>
            </div>
        </div>
        <br /><br /><br /><br />

        <div align="center">
            <h2>

                All the Records of Blood Donors
            </h2>

        </div>




        <br /><br />
        @model List<BloodDonation.Models.blood>
        <table align="center" border="1" width="800">

            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>City</td>
                <td>CellNo</td>

                <td>Gender</td>
                <td>CNIC</td>
                <td>Blood Group</td>

            </tr>
            @foreach (var s in Model)
            {
                <tr>
                    <td>@s.Id</td>
        <td>@s.name_</td>
        <td>@s.city</td>
        <td>@s.cellno</td>

        <td>@s.gender</td>
        <td>@s.cnic</td>
        <td>@s.bgroup</td>
    </tr>

            }

        </table>

        <br /><br /><br /><br /><br />

        <div align="center">
            <h2>

                Enter id to delete a record
            </h2>

          
            <form action="/Admin/del" method="post">
                 
                 

                <input type="number" name="id"/>
                <input type="submit" name="submid" value="Delete Record" />
            </form>
        </div><br /><br /><br /><br /><br /><br /><br /><br />
    </div>
</body></html>